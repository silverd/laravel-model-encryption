{
    "name": "silverd/laravel-model-encryption",
    "description": "A trait to encrypt data models in Laravel, automatically encrypt and decrypt model data overriding getAttribute an setAttribute methods of Eloquent Model.",
    "keywords": ["encrypt", "laravel", "eloquent", "model"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Silverd\\Encryptable\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-openssl": "*",
        "illuminate/database": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Silverd\\Encryptable\\ServiceProvider"
            ]
        }
    }
}
